{
	"name": "@proliferate/web",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "sh -c 'if [ \"$1\" = \"--\" ]; then shift; fi; if [ \"$NEXT_WEB_BUNDLER\" = \"webpack\" ]; then exec next dev --webpack \"$@\"; fi; exec next dev \"$@\"' --",
		"dev:webpack": "sh -c 'if [ \"$1\" = \"--\" ]; then shift; fi; exec next dev --webpack \"$@\"' --",
		"dev:polling": "sh -c 'if [ \"$1\" = \"--\" ]; then shift; fi; WATCHPACK_POLLING=true exec next dev \"$@\"' --",
		"dev:webpack:polling": "sh -c 'if [ \"$1\" = \"--\" ]; then shift; fi; WATCHPACK_POLLING=true exec next dev --webpack \"$@\"' --",
		"build": "next build",
		"start": "next start",
		"typecheck": "tsc --noEmit",
		"lint:elements": "eslint src --max-warnings 0",
		"test": "vitest run src/test/unit",
		"test:watch": "vitest src/test/unit",
		"e2e": "playwright test",
		"e2e:headed": "playwright test --headed"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.71.2",
		"@assistant-ui/react": "^0.11.53",
		"@aws-sdk/client-s3": "^3.970.0",
