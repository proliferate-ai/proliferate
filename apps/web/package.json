		"@radix-ui/react-tabs": "^1.1.13",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@sentry/nextjs": "^10.34.0",
		"@tanstack/react-query": "^5.90.20",
		"@ts-rest/serverless": "^3.52.1",
