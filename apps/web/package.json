		"@nangohq/node": "^0.69.22",
		"@orpc/client": "^1.13.4",
		"@orpc/tanstack-query": "^1.13.4",
		"@proliferate/db": "workspace:*",
		"@proliferate/environment": "workspace:*",
		"@proliferate/gateway-clients": "workspace:*",
		"@proliferate/logger": "workspace:*",
		"@proliferate/queue": "workspace:^",
		"@proliferate/services": "workspace:*",
		"@proliferate/shared": "workspace:*",
		"@proliferate/triggers": "workspace:*",
		"@radix-ui/react-alert-dialog": "^1.1.15",
		"@radix-ui/react-avatar": "^1.1.11",
