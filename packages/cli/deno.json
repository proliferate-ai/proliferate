{
	"name": "@proliferate/cli",
	"version": "0.2.0",
	"exports": "./src/index.ts",
	"unstable": ["sloppy-imports"],
	"tasks": {
		"dev": "deno run --allow-all src/index.ts",
		"compile": "deno compile --allow-all --output=binaries/proliferate src/index.ts",
		"compile:all": "deno task compile:darwin-arm64 && deno task compile:darwin-x64 && deno task compile:linux-x64 && deno task compile:linux-arm64",
		"compile:darwin-arm64": "deno compile --allow-all --target=aarch64-apple-darwin --output=binaries/proliferate-darwin-arm64 src/index.ts",
		"compile:darwin-x64": "deno compile --allow-all --target=x86_64-apple-darwin --output=binaries/proliferate-darwin-x64 src/index.ts",
		"compile:linux-x64": "deno compile --allow-all --target=x86_64-unknown-linux-gnu --output=binaries/proliferate-linux-x64 src/index.ts",
		"compile:linux-arm64": "deno compile --allow-all --target=aarch64-unknown-linux-gnu --output=binaries/proliferate-linux-arm64 src/index.ts"
	},
	"imports": {
		"@proliferate/gateway-clients": "../gateway-clients/src/index.ts",
		"@proliferate/shared": "../shared/src/index.ts",
		"chalk": "npm:chalk@^5.3.0",
		"open": "npm:open@^10.0.0",
		"ora": "npm:ora@^8.0.0",
		"jose": "npm:jose@^5.2.0",
		"zod": "npm:zod@^3.22.3",
		"ws": "npm:ws@^8.18.0",
		"bullmq": "npm:bullmq@^5.30.1",
		"@ts-rest/core": "npm:@ts-rest/core@^3.52.1",
		"e2b": "npm:e2b@^2.10.2",
		"modal": "npm:modal@^0.6.0"
	},
	"nodeModulesDir": "auto",
	"compilerOptions": {
		"lib": ["deno.ns", "deno.unstable", "ES2022"]
	}
}
